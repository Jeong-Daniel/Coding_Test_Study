   "source": [
    "num = int(input())\n",
    "a_list = list(map(int,input().split()))\n",
    "b_list = list(map(int,input().split()))\n",
    "\n",
    "a_list.sort()\n",
    "b_list.sort(reverse=True)\n",
    "\n",
    "ans = 0\n",
    "for i in range(num):\n",
    "    ans += (a_list[i]*b_list[i])\n",
    "\n",
    "print(ans)"
