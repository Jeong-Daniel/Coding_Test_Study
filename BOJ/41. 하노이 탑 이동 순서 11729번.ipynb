    "print(2**num-1)\n",
    "def hinoi(a,b,n):\n",
    "    if n==1:\n",
    "        print(a,b)\n",
    "        return\n",
    "    hinoi(a,6-a-b,n-1)\n",
    "    print(a,b)\n",
    "    hinoi(6-a-b,b,n-1)\n",
    "    \n",
    "hinoi(1,3,num)"
