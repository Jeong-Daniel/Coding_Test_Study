   "source": [
    "num = int(input())\n",
    "num_list = list(map(int,input().split()))\n",
    "dp = [1]*num\n",
    "for i in range(num):\n",
    "    for j in range(i):\n",
    "        if num_list[j] < num_list[i]:\n",
    "            dp[i] = max(dp[i], dp[j]+1)\n",
    "print(max(dp))"
   ]
